        "api",
        "privateToken"
      ]
    },
    "ldap": {
      "type": "object",
      "properties": {
        "url": {
          "type": "string"
        },
        "baseDN": {
          "type": "string"
        },
        "username": {
          "type": "string"
        },
        "password": {
          "type": "string"
        }
      },
      "required": [
        "url",
        "baseDN",
        "username",
        "password"
      ]
    }
  }
}
